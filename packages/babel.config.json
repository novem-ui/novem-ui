{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "targets": {
          "esmodules": true
        }
      }
    ],
    "@babel/preset-react",
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ],
    "minify"
  ],
  "ignore": ["**/node_modules/", "**/dist/", "**/*.stories.tsx", "**/*.spec.ts", "**/*.spec.tsx"],
  "comments": false
}
